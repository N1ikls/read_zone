{
  "extends": ["../.nuxt/tsconfig.server.json"],
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "noEmit": true,
    "jsx": "preserve",
    "lib": ["ES2016", "DOM", "DOM.Iterable"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "types": ["node", "nitropack"],
    "paths": {
      "@/*": ["./*"],
      "#config": ["../config/config"],
      "~/utils/*": ["./utils/*"]
    }
  },
  "exclude": ["node_modules"]
}
